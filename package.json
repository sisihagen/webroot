{
  "name": "silviosiefke.com",
  "version": "0.0.75",
  "description": "Private Website ",
  "main": "index.html",
  "scripts": {
    "build": "./node_modules/.bin/gulp build",
    "deploy": "./bin/deploy.sh",
    "git": "./node_modules/.bin/gulp git",
    "watch:css": "./node_modules/.bin/onchange './static/static/scss/*.scss' -- npm run scss",
    "watch:js": "./node_modules/.bin/onchange './static/static/js/site.js' -- npm run build:js",
    "concat": "./node_modules/.bin/concat ./node_modules/jquery/dist/jquery.js ./node_modules/flexibility/flexibility.js ./static/static/js/app.js -o ./static/static/js/site.js",
    "uglify": "./node_modules/.bin/uglifyjs ./static/static/js/site.js -o ./static/static/js/site.js",
    "scss": "./node_modules/.bin/node-sass --output-style compressed -o ./static/static/css ./static/static/scss/layout.scss",
    "postcss": "./node_modules/.bin/postcss -u autoprefixer postcss-opacity postcss-flexibility cssnano -r ./static/static/css/*",
    "de": "./node_modules/.bin/html-minifier --collapse-whitespace --remove-comments --remove-optional-tags --remove-redundant-attributes --remove-script-type-attributes --remove-tag-whitespace --quote-character \" --file-ext xml txt --input-dir ./public/build/de/ --output-dir ./public/dest/silviosiefke.de/htdocs",
    "en": "./node_modules/.bin/html-minifier --collapse-whitespace --remove-comments --remove-optional-tags --remove-redundant-attributes --remove-script-type-attributes --remove-tag-whitespace --quote-character \" --file-ext xml txt --input-dir ./public/build/en/ --output-dir ./public/dest/silviosiefke.com/htdocs",
    "fr": "./node_modules/.bin/html-minifier --collapse-whitespace --remove-comments --remove-optional-tags --remove-redundant-attributes --remove-script-type-attributes --remove-tag-whitespace --quote-character \" --file-ext xml txt --input-dir ./public/build/fr/ --output-dir ./public/dest/silviosiefke.fr/htdocs",
    "ru": "./node_modules/.bin/html-minifier --collapse-whitespace --remove-comments --remove-optional-tags --remove-redundant-attributes --remove-script-type-attributes --remove-tag-whitespace --quote-character \" --file-ext xml txt --input-dir ./public/build/ru/ --output-dir ./public/dest/silviosiefke.ru/htdocs",
    "build:css": "npm run scss && npm run postcss",
    "build:js": "npm run concat && npm run uglify",
    "build:html": "npm run de && npm run en && npm run fr && npm run ru"
  },
  "repository": "github.com/sisihagen/webroot",
  "keywords": [
    "Hugo",
    "node",
    "nodejs",
    "npm",
    "Normalize",
    "static-site",
    "sass",
    "javascript",
    "web development"
  ],
  "author": "Silvio Siefke <siefke@mail.ru> (http://www.silviosiefke.com)",
  "license": "SimPL-2.0",
  "devDependencies": {
    "@hapi/hoek": "^8.2.1",
    "autoprefixer": "^9.6.0",
    "child_process": "^1.0.2",
    "cssnano": "^4.1.10",
    "del": "^4.1.1",
    "extend": "^3.0.2",
    "flexibility": "^2.0.1",
    "fs-extra": "^8.0.1",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^6.1.0",
    "gulp-bump": "^3.1.3",
    "gulp-changed": "^4.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-exec": "^3.0.2",
    "gulp-git": "^2.9.0",
    "gulp-git-push": "^2.0.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^8.0.0",
    "gulp-prompt": "^1.2.0",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify": "^3.0.2",
    "html5shiv": "^3.7.3",
    "jquery": "^3.4.1",
    "merge-stream": "^2.0.0",
    "minimatch": "^3.0.4",
    "node-sass": "^4.12.0",
    "npm-check-updates": "^3.1.11",
    "picturefill": "^3.0.3",
    "postcss-cli": "^6.1.3",
    "postcss-flexibility": "^2.0.0",
    "postcss-opacity": "^5.0.0",
    "static-eval": "^2.0.2",
    "vinyl-buffer": "^1.0.1"
  },
  "dependencies": {
    "concat": "^1.0.3",
    "onchange": "^6.1.0",
    "parallelshell": "^3.0.2"
  }
}
